{
    "name": "itinerare/alcyone",
    "type": "project",
    "description": "A framework for simple private image hosting.",
    "keywords": [
        "image hosting"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^3.1",
        "laracasts/flash": "^3.2",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^10.10",
        "laravel/helpers": "^1.4",
        "laravel/tinker": "^2.8",
        "laravelcollective/html": "^6.4",
        "spatie/flysystem-dropbox": "^3.0.0",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-honeypot": "^4.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "marcocesarato/php-conventional-changelog": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sniff": [
            "./vendor/bin/pint --test"
        ],
        "lint": [
            "./vendor/bin/pint"
        ],
        "test-coverage": [
            "./vendor/bin/phpunit --coverage-text"
        ],
        "changelog": [
            "./vendor/marcocesarato/php-conventional-changelog/conventional-changelog"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.5.1"
}